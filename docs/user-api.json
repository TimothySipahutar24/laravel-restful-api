{
    "openapi": "3.0.3",
    "info": {
        "title": "User API",
        "description": "User API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8080"
        }
    ],
    "paths": {
        "/api/users/register": {
            "post": {
                "description": "Register new user",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {},
                                }
                            }
                        }
                    }
                },
            }
        },
        "api/users/login": {
            "post": {
                "description": "Login user"
            }
        },
        "api/users/current": {
            "get": {
                "description": "Get current user"
            },
            "patch": {
                "description": "Update current user"
            }
        }
    }
}
